{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Personal Consumption Expenditures Over Time",
  "data": {
    "url": "https://raw.githubusercontent.com/aydan-ui/aydan-ui.github.io/main/PCE.json",
    "format": {"type": "json"}
  },
  "mark": "line",
  "encoding": {
    "x": {"field": "date", "type": "temporal", "title": "Date"},
    "y": {"field": "value", "type": "quantitative", "title": "PCE (Billions USD)"},
    "tooltip": [{"field": "date", "type": "temporal"}, {"field": "value", "type": "quantitative"}]
  },
  "width": 300,
  "height": 200
}
