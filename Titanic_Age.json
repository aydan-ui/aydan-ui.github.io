{
  "title": {
    "text": "Passenger Age Distribution on the Titanic",
    "subtitle": "Source: Titanic Dataset (Seaborn)",
    "fontSize": 16,
    "fontWeight": "bold"
  },
  "x_axis": {
    "field": "Age",
    "type": "quantitative",
    "bin": {"maxbins": 30},
    "title": "Age (Years)"
  },
  "y_axis": {
    "aggregate": "count",
    "type": "quantitative",
    "title": "Frequency",
    "scale": {"domain": [0, 35]}
  },
  "tooltip": [
    {"field": "count", "type": "quantitative"},
    {"field": "Age", "type": "quantitative"}
  ],
  "properties": {
    "width": 600,
    "height": 400
  },
  "mark": "bar"
}
